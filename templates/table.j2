
<table border="1" style="width:100%">
    {% if title %}
    <tr>
        <td style="text-align:center" colspan={{col_names_num}}> {{title}} </td>
    </tr>
    {% endif%}

    <tr>
    {% for colname in col_names%}
        <th> {{colname}} </th>
    {% endfor %}  
    </tr>  
    {% for row in arranged_table %}
    {% if numRow == None %}
    <tr>
        {% for column in row %}
        <td>{{column}}</td>
        {% endfor %}
    </tr>
    {% elif loop.index <= numRow %}
    <tr>
        {% for column in row %}
        <td>{{column}}</td>
        {% endfor %}
    </tr>
    {% endif %}
    {% endfor %}
</table>